<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="person" class="com.example.spring.HelloWorldSpring.dto.Person">
	<constructor-arg value="Ericka Montero!"></constructor-arg>
	<constructor-arg value="27"></constructor-arg>
	</bean>
	<bean id="employee" class="com.example.spring.HelloWorldSpring.dto.Employee">
	<property name="salary" value="1236456"></property>
	<property name="name" value="Ericka Montero"></property>
	</bean>
</beans>
